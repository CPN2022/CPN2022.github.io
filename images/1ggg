                        <script>
            (function() {
                var url=encodeURIComponent(document.location);
                var setting={
                            homeURL : 'https://photo.xuite.net',
                            addURL : 'https://photo.xuite.net/_my2/photo/html5uploader',
                            manageURL : 'https://photo.xuite.net/_my2/',
                            loginURL : 'https://photo.xuite.net/@login?furl=' + url,
                            logoutURL : 'https://photo.xuite.net/@logout?furl=' + url,
                            recommendURL : '#', // optional
                            recommendClickHandler : function() {
                                    var url = 'https://photo.xuite.net';;
                                    var rec_type = '';
                                    var link = '';

                                    if ( link = location.pathname.match(/\/([0-9a-zA-z\_\.]*)\/([0-9]+)\/[0-9]+\.jpg([0-9a-zA-z\_\.]*)/) ) {
                                        rec_type = 'pic';
                                        recommend(url+link[0],'photo',rec_type);
                                    } else if ( link = location.pathname.match(/\/([0-9a-zA-z\_\.]*)\/([0-9]+)/) ) {
                                        rec_type = 'plist';
                                        recommend(url+link[0],'photo',rec_type);
                                    } else if ( link = location.pathname.match(/\/([0-9a-zA-z\_\.]*)/) ) {
                                        rec_type = 'alist';
                                        recommend(url+link[0],'photo',rec_type);
                                    } else
                                        alert("連結有問題!!!");
                            },
                            favorURL : '#', // optional
                            favorClickHandler : function() {
                                var url = "https://my.xuite.net/service/bookmark/my/bmAdd.php";
                                url+= "?url="+encodeURIComponent(document.URL);
                                url+= "&name="+encodeURIComponent(document.title);
                                var info = "width=500,height=400,left=75,top=20,resizable=yes,scrollbars=yes,status=yes";
                                window.open(url, "favorite", info);
                            },
                            sysopURL : 'https://blog.xuite.net/xuite.net/xuite',
                            searchPrefix : 'https://xuite.net/search_index.php?query='// + encodeURIComponent($('.top-toolbar-keyword').val())
                        };
                var page=1;
                if (location.pathname.match(/([0-9a-zA-z\_\.]*)\/([0-9]+)\/[0-9]+\.jpg\/sizes/)) {
                    page=3;
                } else if (location.pathname.match(/([0-9a-zA-z\_\.]*)\/([0-9]+)\/[0-9]+\.jpg/)) {
                    page=2;
                }

                if (page<=3) {
                    setting.abuseURL="/#";
                }
                var pushLink="";
                var pushTitle="";
                if (page>=2) {
                    setting.pushImage ='https://2.share.photo.xuite.net/wuyungyuan/12a1d79/13853965/1179753580_l.jpg';
                    pushLink = 'https://photo.xuite.net/wuyungyuan/13853965/1219.jpg/';
                    pushTitle = 'IMG_20170815_152249.jpg - 未分類相簿 - wuyungyuan的相簿 @ 隨意窩 Xuite 相簿';
                } else {
                                        pushTitle = '的相簿 @ 隨意窩 Xuite 相簿';
                    pushLink ='https://photo.xuite.net/';
                                    }
                setting.pushLink = pushLink;
                setting.pushTitle = pushTitle;

                
                var topToolbar = new XUI.Widgets.TopToolbar(document.getElementById('top-toolbar1'), setting);
                topToolbar.render();
                                    if($('#ysmkeys').val() != null ){
                        var ysmkeyword = $('#ysmkeys').val().split(",");
                        var ysmtitle = '';
                        ysmtitle = "未分類相簿";
                        if(ysmkeyword.length > 0 || ysmtitle){
                            setTimeout(function() {
                                $('.top-toolbar').data('top-toolbar').setAD({
                                    keywords: ysmkeyword,
                                    title: ysmtitle,
                                    label_id: ysmtitle,
                                    label_name: ''
                                });
                            }, 2000);
                        }
                    }
                
                function recommend(url, channel, type, content){
                    content = content || "";    // 給content的預設值為""
                    var dataString = "url="+url+"&uid="+$.cookie('xuite_id')+"&channel="+channel+"&type="+type+"&content="+content;
                    var url = "https://my.xuite.net/service/rec/my/cache_recommend.php?" + dataString;
                    $.getJSON(url + "&jsoncallback=?", function(data){
                        alert(data.success);
                    });
                }

                if ($('#socialbar').length>0) {
                    (new XUI.Widgets.Push(
                        $("#socialbar").get(0),
                        {
                            imageURL : 'https://2.share.photo.xuite.net/wuyungyuan/12a1d79/13853965/1179753580_l.jpg', // optional
                            link : 'https://photo.xuite.net/wuyungyuan/13853965/1219.jpg/' // optional
                        }
                    )).render();
                }
                            	if ( $('#fb_like1').length>0 ) {
                	(new XUI.Widgets.Like( $('#fb_like1').get(0),
                	   {
                            URL : pushLink
                        }
                    )).render();
            	}

            	if ( $('#fb_like2').length>0 ) {
                    (new XUI.Widgets.Like( $('#fb_like2').get(0),
                        {
                            mode : 'face',
                            URL : pushLink
            //                URL : 'http://xuite.net/' // optional, default indow.location.href.split('#')[0]
                        }
                    )).render();
            	}
                            })();
